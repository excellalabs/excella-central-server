{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "Email": {
    "dataSource": "email-source"
  },
  "User": {
    "dataSource": "db",
    "options": {
      "emailVerificationRequired": true
    }
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "account": {
    "dataSource": "mongo-test",
    "public": true,
    "options": {
      "emailVerificationRequired": true
    }
  },
  "profile": {
    "dataSource": "mongo-test",
    "public": true
  }
}
